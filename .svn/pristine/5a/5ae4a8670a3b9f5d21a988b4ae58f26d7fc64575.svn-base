<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">

	<select id="sampleList" resultType="capstone.sejong.question.SampleDTO">
		select * from my_table
	</select>

	<insert id="insert" parameterType="capstone.sejong.question.QuestionDTO">
		INSERT INTO catchme_question
		values(default,#{title},#{content},#{questiontype},#{email})
	</insert>
</mapper>